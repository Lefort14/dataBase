  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx start.ts",
    "dev:launcher": "nodemon --watch launcher/src --exec \"electron --no-sandbox -r tsx launcher/src/electron.ts\"",
    "start": "node prod/start.js",
    "launcher": "electron --no-sandbox prod/launcher/src/electron.js",
    "build": "npx tsc && npm run build:assets && npm run build:boostrap",
    "build:assets": "npx cpy '**/views/**/*' '**/public/**/*' prod",
    "build:boostrap": "npx cpy 'node_modules/bootstrap/dist/**/*' prod/public/boostrap"
  },
